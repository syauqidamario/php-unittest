{
    "name": "syauqi/php-unittest",
    "description": "Learn to create a PHP unit test.",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Syauqi\\PhpUnittest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Syauqi\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "yes"
        }
    ],
    "require": {},
    "require-dev":{
        "phpunit/phpunit": "9.5.5"
    },
    "scripts": {
        "test" : "phpunit --configuration phpunit.xml",
        "test:product": "phpunit --configuration phpunit.xml --testsuite product",
        "test:counter": "phpunit --configuration phpunit.xml --testsuite counter"
    }
}
